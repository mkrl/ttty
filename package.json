{
  "name": "ttty",
  "version": "0.5.0",
  "files": [
    "dist",
    "src"
  ],
  "module": "./dist/ttty.es.js",
  "browser": "./dist/ttty.iife.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ttty.es.js"
    }
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build && npm run emit",
    "emit": "tsc -m amd -t ESNext -d --isolatedModules false  --moduleResolution node --esModuleInterop --outFile dist/index.js src/index.ts"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.6",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "typescript": "^4.3.2",
    "vite": "^2.5.4"
  }
}
